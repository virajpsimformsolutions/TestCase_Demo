{
  "name": "TodoApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "new": "yarn react-native run-android - active-arch-only",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest --verbose --coverage",
